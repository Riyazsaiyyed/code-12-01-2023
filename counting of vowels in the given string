#include<iostream>
using namespace std;
int main()
{
 char s[10];
 int count=0,n,i;
cout<<"enter the string\n";
cin>>s;
 for(i=0;s[i]!='\0';i++)
 {
 if(s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u'||s[i]=='A'||s[i]=='E'||s[i]=='I'||s[i]=='O'||s[i]=='U')
 {
  count++;
 }
 }
 cout<<"the count of vowels in the given string is "<<count<<endl;
}
